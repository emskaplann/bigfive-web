{
  "name": "bigfive-web",
  "version": "3.0.2",
  "description": "Website for testing five factor model of personality",
  "author": "Jonas Enge <jonas.enge@gmail.com> (https://github.com/maccyber)",
  "private": true,
  "license": "MIT",
  "keywords": [
    "bigfive",
    "personality"
  ],
  "precommit": "yarn lint",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "setup": "yarn install --frozen-lockfile && cd api && yarn install --frozen-lockfile",
    "refresh": "rm -rf node_modules && rm yarn.lock && cd api && rm -rf node_modules && rm yarn.lock && cd .. && npm run setup",
    "generate": "nuxt generate",
    "test": "yarn lint && ava -v",
    "debug-build": "nuxt build -a",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@aceforth/nuxt-optimized-images": "1.3.0",
    "bigfivequestionscustom-2020": "5.2.7",
    "@nuxt/content": "1.10.0",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/pwa": "3.2.2",
    "@nuxtjs/sitemap": "2.4.0",
    "amplitude-js": "7.3.0",
    "apexcharts": "3.22.0",
    "b5-result-text-custom": "^4.1.6",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.1",
    "nuxt": "2.14.7",
    "nuxt-amplitude": "0.1.2",
    "nuxt-i18n": "6.15.4",
    "nuxt-webfontloader": "1.1.0",
    "svg-country-flags": "1.2.9",
    "vue-amplitude": "1.0.3",
    "vue-apexcharts": "1.6.0",
    "vue-confetti": "2.2.1",
    "vue-template-compiler": "2.6.12"
  },
  "devDependencies": {
    "@mdi/js": "5.7.55",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/vuetify": "1.11.2",
    "ava": "3.13.0",
    "eslint": "7.11.0",
    "eslint-config-standard": "15.0.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.2",
    "eslint-plugin-vue": "7.1.0"
  }
}
